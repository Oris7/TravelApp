{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "PostgreSQL": "Host=<HOST>;Database=<DB>;Username=<USER>;Password=<PASSWORD>;Port=<PORT>;SslMode=require;TrustServerCertificate=true"
  },
